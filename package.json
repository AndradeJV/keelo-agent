{
  "name": "keelo",
  "version": "1.0.0",
  "description": "Keelo - Autonomous Functional PR Reviewer & QA Agent",
  "main": "dist/main.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/main.ts",
    "dev:web": "npm --prefix web run dev",
    "dev:all": "npm run dev & npm run dev:web",
    "build": "tsc",
    "start": "node dist/main.js",
    "test": "tsx --test src/**/*.test.ts",
    "typecheck": "tsc --noEmit",
    "lint": "tsc --noEmit"
  },
  "keywords": [
    "github",
    "pr-review",
    "qa",
    "testing",
    "automation",
    "llm",
    "openai",
    "anthropic"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.72.0",
    "@octokit/auth-app": "^6.0.3",
    "@octokit/rest": "^20.0.2",
    "@octokit/webhooks": "^12.0.10",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "google-auth-library": "^10.6.1",
    "jsonwebtoken": "^9.0.3",
    "nodemailer": "^8.0.1",
    "openai": "^4.28.0",
    "pg": "^8.18.0",
    "pino": "^8.17.2",
    "pino-pretty": "^10.3.1",
    "socket.io": "^4.8.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.11.5",
    "@types/nodemailer": "^7.0.11",
    "@types/pg": "^8.16.0",
    "@types/ws": "^8.18.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "overrides": {
    "cross-spawn": "^7.0.6",
    "glob": "^10.5.0",
    "tar": "^6.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
